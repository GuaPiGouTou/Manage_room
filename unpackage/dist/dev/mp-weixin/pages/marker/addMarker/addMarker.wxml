<view class="container"><view class="section"><view class="section-title">位置信息</view><button class="location-btn" bindtap="{{a}}"><text>选择地图坐标</text></button><view wx:if="{{b}}" class="location-info"><text class="info-label">位置名称：</text><text class="info-value">{{c}}</text><view class="coords-container"><view class="coord-item"><text class="info-label">经度：</text><text class="info-value">{{d}}</text></view><view class="coord-item"><text class="info-label">纬度：</text><text class="info-value">{{e}}</text></view></view></view></view><view class="section"><view class="section-title">联系方式</view><input class="input" placeholder="微信" value="{{f}}" bindinput="{{g}}"/><input class="input" placeholder="手机号" type="number" value="{{h}}" bindinput="{{i}}"/></view><view class="section"><view class="section-title">房源信息</view><input class="input" placeholder="房源标题" value="{{j}}" bindinput="{{k}}"/><input class="input" placeholder="具体地点" value="{{l}}" bindinput="{{m}}"/><input class="input" placeholder="面积（平方米）" value="{{n}}" bindinput="{{o}}"/><view><text style="margin-left:10rpx;margin-bottom:10rpx;font-weight:500">房间数量</text><input class="input" placeholder="" type="number" value="{{p}}" bindinput="{{q}}"/></view></view><button class="submit-btn" bindtap="{{r}}">提交房源信息</button><button class="submit-btn" bindtap="{{s}}">下一步</button></view>