<view class="property-container"><view class="header"><text class="title">{{a}}</text><text class="location">{{b}}</text></view><view class="info-card base-info"><view class="card-header"><text class="card-title">小区基本信息</text></view><view class="info-grid"><view class="info-item"><text class="info-label">地址:</text><text class="info-value">{{c}}</text></view><view class="info-item"><text class="info-label">面积:</text><text class="info-value">{{d}}㎡</text></view><view class="info-item"><text class="info-label">联系电话:</text><text class="info-value">{{e}}</text></view><view class="info-item"><text class="info-label">微信:</text><text class="info-value">{{f}}</text></view><view><button type="primary" bindtap="{{g}}">添加房间</button></view></view></view><view class="info-card room-list"><view class="card-header"><text class="card-title" bindtap="{{i}}">房源信息 ({{h}}间)</text></view><view wx:for="{{j}}" wx:for-item="room" wx:key="j" class="room-card"><view class="room-header"><text class="room-number">{{room.a}}</text><text class="room-type">{{room.b}}</text></view><view class="room-details"><view class="detail-item"><text class="detail-label">地址:</text><text class="detail-value">{{room.c}}</text></view><view class="detail-item"><text class="detail-label">面积:</text><text class="detail-value">{{room.d}}㎡</text></view><view class="detail-item"><text class="detail-label">支付方式:</text><text class="detail-value">{{room.e}}</text></view><view class="detail-item"><text class="detail-label">家具配置:</text><text class="detail-value">{{room.f}}</text></view><view><button type="warn" bindtap="{{room.g}}">删除房间</button></view></view><view wx:if="{{room.h}}" class="video-section"><text class="section-title">视频预览</text><view class="video-thumbnails"><view wx:for="{{room.i}}" wx:for-item="video" wx:key="c" class="video-thumb" bindtap="{{video.d}}"><video class="video-icon" src="{{video.a}}"></video><text class="video-index">视频{{video.b}}</text></view></view></view></view></view></view>